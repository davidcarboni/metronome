{
  "expo": {
    "name": "metronome",
    "slug": "metronome",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "metronome",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.carboni.metronome",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "androidStatusBar": {
      "hidden": true,
      "backgroundColor": "#ffffff"
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "io.carboni.metronome"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/DmSans-regular.ttf",
            "./assets/fonts/DmSans-700.ttf",
            "./assets/fonts/DmSans-900.ttf",
            "./assets/fonts/PublicSans-regular.ttf",
            "./assets/fonts/PublicSans-700.ttf",
            "./assets/fonts/PublicSans-900.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d19efddc-0d56-41c8-a653-c2cb97cc35bb"
      }
    }
  }
}
